    "@tailwindcss/postcss": "^4.1.17",
    "rollup-plugin-postcss": "^4.0.2",
    "@types/node": "^24.10.1",
    "@types/react": "^19.2.7",
    "babel-plugin-react-compiler": "^1.0.0",
    "publint": "^0.3.15",
    "tailwindcss": "^4.1.17",
